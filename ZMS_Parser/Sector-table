            <script type="text/javascript">
            $('#sector_industry_tabs li a').click(function(){
        		if ($(this).parent('li').attr("class").indexOf('selected') != -1) {
        			return false;
        		}
        		$(this).addClass('nohand');
        		var	idName = $(this).attr('href');
        		$('.si-tab-container').hide();
        		$('#sector_industry_tabs li').removeClass('selected');
        		$(this).parent('li').addClass('selected');
        		$(idName).show();
        		if(idName == "#tabDataIndustry")
        		{
        			sectorTabFlag = 0;
        			$('#tab_sector').removeClass('nohand');
        			$(this).parents().next('a').children('img').attr('alt','Compare Industry Performance');
        			$(this).parents().next('a').children('img').attr('title','Compare Industry Performance');
        			setIndustriesData();

        		}
        		else
        		{
        			sectorTabFlag = 1;
        			$('#tab_industry').removeClass('nohand');
        			$(this).parents().next('a').children('img').attr('alt','Compare Sector Performance');
        			$(this).parents().next('a').children('img').attr('title','Compare Sector Performance');
        			performanceSectorPage.getPerformanceSectorHtml("","");
        		}



        		return false;
        	});




	    	    	$('#tab_sector').click();



			(function(){
				$('#tab_sector').bind('click', function() {
					//trackContentChangeAsPV('Tab','Snapshot - Sector');
				});
				$('#tab_industry').bind('click', function() {
					//trackContentChangeAsPV('Tab','Snapshot - Industry');
				});
			})();

            </script>
